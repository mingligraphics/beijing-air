\n\n.text-center {\n    text-align:center;\n}\n\nh1.headline {\n    position:relative;\n    font-size: 4rem;\n    color:white;\n    line-height: 1.1;\n    top: 80px;\n    font-weight: 800;\n\n}\n\nh2.subhead {\n    position:relative;\n    font-size: 1.8rem;\n    line-height: 2rem;\n    margin-bottom:0;\n    font-weight: 400;\n    top: 65px;\n}\n\np.body-text {\n    font-size: 1.1rem;\n    line-height: 1.4;\n    margin-top:0;\n    margin-bottom:15px;\n}\n\nh3.section-head {\n    font-size:2rem;\n    margin:40px 0 10px 0;\n    font-weight:700;\n    line-height:1.3;\n}\n\np.byline {\n    position:relative;\n    font-size: 1.1rem;\n    line-height: 0.75;\n    margin-top: 1.5rem;\n    font-weight: 500;\n    top: 70px;\n}\n\np.date {\n    position:relative;\n    font-size: 0.85rem;\n    line-height: 1;\n    margin-top: 10px;\n    font-weight:400;\n    text-transform:uppercase;\n    letter-spacing:0.5px;\n    top: 65px;\n}\n\n\np.note {\n    font-size:0.85rem;\n}\n\n/* chart styles */\n\n.chart-section {\n    width:100%;\n    max-width:1200px;\n    padding:20px 0px;\n}\n\n.story-body .chart-section {\n    padding-top:0px;\n}\n\n\n.chart-row {\n    display:flex;\n    justify-content: space-between;\n    margin-top:20px;\n}\n\np.chart-title {\n    font-size:1.3rem;\n    font-weight:700;\n    max-width:700px;\n    margin:0px auto 5px auto;\n}\np.chart-subhead {\n    margin:0 auto;\n    max-width:700px;\n    font-size:1rem;\n    margin-bottom:15px;\n}\n\np.chart-label {\n    margin:0 0 5px 0;\n    font-size:0.85rem;\n    text-transform:uppercase;\n    font-weight:600;\n}\n\n.chart-row .chart {\n    width:100%;\n    padding-right:15px;\n    min-width:calc(25% - 15px);\n\n}\n\n\n\np.chart-source {\n    font-size:0.7rem;\n    margin:0;\n}\n\n/*mapbox */\n\n#map { \n    /* position: absolute; \n    top: 0; \n    bottom: 0;  */\n    height: 90vh;\n    width: 100%;\n    max-width: 640px;\n    margin:auto;\n};\n\n.mapboxgl-popup {\n    max-width: 400px;\n    font: 12px/20px 'Helvetica Neue', Arial, Helvetica, sans-serif;}\n\n\n.mapboxgl-map {\n    font: 12px/20px 'Helvetica Neue', Arial, Helvetica, sans-serif;\n    overflow: hidden;\n    position: absolute;\n    -webkit-tap-highlight-color: rgba(0, 0, 0, 0);\n    text-align: left;\n    }\n\n .detector {\n    display: flex;\n    flex-wrap: wrap;\n    width: 60px;\n      }\n\n .group {\n    width: 10px;\n    height: 40px;\n    margin: 2px;\n      }\n/* photos */\n\n.photo-section {\n    padding:30px 0px;\n    width:100%;\n    margin:auto;\n    display:flex;\n    max-width:1200px;\n}\n\n.photo {\n    width:100%;\n    padding-right:15px;\n}\n.photo img {\n    width:100%;\t\n}\n\np.caption {\n    font-size:0.9rem;\n    margin:0;\n}\n\n\n\n/* mobile */\n\n@media (max-width:700px) {\n\n    .chart-row {\n        display:block;\n    }\n\n    .chart-row .chart {\n        padding:0;\n    }\n\n    .photo-section {\n        display:block;\n    }\n\n    .photo {\n        padding:0;\n    }\n\n}\n"},"lineCount":null}},"error":null,"hash":"db3790664ac06509d8f7695c2aef1d07","cacheData":{"env":{}}}