ource":"scrollytelling.css","name":null,"original":{"line":102,"column":2},"generated":{"line":1,"column":995}},{"source":"scrollytelling.css","name":null,"original":{"line":103,"column":6},"generated":{"line":1,"column":1013}},{"source":"scrollytelling.css","name":null,"original":{"line":104,"column":2},"generated":{"line":1,"column":1038}},{"source":"scrollytelling.css","name":null,"original":{"line":105,"column":2},"generated":{"line":1,"column":1039}},{"source":"scrollytelling.css","name":null,"original":{"line":106,"column":6},"generated":{"line":1,"column":1062}},{"source":"scrollytelling.css","name":null,"original":{"line":107,"column":2},"generated":{"line":1,"column":1076}},{"source":"scrollytelling.css","name":null,"original":{"line":108,"column":0},"generated":{"line":1,"column":1077}}],"sources":{"scrollytelling.css":"/* \n\nBegin responsive design and scrollytelling\n\n*/\n\nsvg {\n  display: block;\n}\n\n/* Scrollytelling sections can be wider than the normal 700px width */\n.scrollytelling.story-body {\n  max-width: 960px;\n}\n\n.scrollytelling {\n  display: flex;\n  flex-wrap: wrap;\n}\n\n/* Everything inside of scrollytelling gets normal full width... */\n.scrollytelling > * {\n  flex-basis: 100%;\n  flex-shrink: 0;\n}\n\n/* ...except the .steps and .chart! They'll share a row, sometimes */\n.scrollytelling > .steps,\n.scrollytelling > .chart {\n  flex-basis: 20%;\n  flex-shrink: 1;\n  flex-grow: 1;\n}\n\n/* The bigger 'flex-grow' the more space it takes up */\n.scrollytelling > .chart {\n  flex-grow: 8;\n  overflow: hidden;\n}\n\n.scrollytelling .chart {\n  margin-right: 15px;\n  position: sticky;\n  top: 0;\n  z-index: 0;\n  height: 100vh;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n}\n\n.scrollytelling .chart svg {\n  margin: auto;\n}\n\n.scrollytelling .chart img {\n  width: 100wh;\n}\n\n.scrollytelling .step {\n  max-width: 50rem;\n  margin: auto;\n  margin-bottom: 75vh;\n  padding: 10px;\n  background-color: white;\n  color:white;\n  pointer-events: auto;\n}\n\n.scrollytelling .step *:first-child {\n  margin-top: 0;\n  padding-top: 0;\n}\n\n.scrollytelling .step *:last-child {\n  margin-bottom: 0;\n  padding-bottom: 0;\n}\n\n#step15{\n  background-color:#d7191c\n}\n#step16{\n  background-color:#2b83ba\n}\n#step17{\n  background-color:#fdae61\n}\n#step18{\n  background-color:#abdda4\n}\n\n/* This is a weird spacing fix */\n.scrollytelling .steps {\n  z-index: 10;\n  padding-bottom: 1vh;\n  pointer-events: none;\n}\n\n/* This is the point where it collapses */\n@media (max-width:900px) {\n  .scrollytelling > * {\n      flex-basis: 100% !important;\n  }\n  .scrollytelling .chart {\n      margin-right: 0;\n  }\n}\n\n/* \n\nEnd scrollytelling \n\n*/"},"lineCount":null}},"error":null,"hash":"54e32e16d365ea01b05c8a8b9d71a9d9","cacheData":{"env":{}}}